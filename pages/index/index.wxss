.banner-swiper-container {
  width: 750rpx;
  height: 400rpx;
}
.vol-banner-swiper-item {
  box-sizing: border-box;
}
.vol-banner-card {
  height: 400rpx;
}
.dots {
  position: absolute;
  /* left: -464rpx; */
  right: 48rpx;
  bottom: 60rpx;
  display: flex;
  justify-content: center;
}
.dots .dot {
  margin: 0 8rpx;
  width: 10rpx;
  height: 10rpx;
  background: #fff;
  border-radius: 50%;
  transition: all 0.6s;
  opacity: 0.3;
}
.dots .dotactive {
  width: 48rpx;
  height: 10rpx;
  border-radius: 5rpx;
  background: #ffffff;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  box-sizing: border-box;
  padding-bottom: 40rpx;
}